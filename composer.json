{
    "name": "zen-studio/joint-buffer",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "protobuf"
    ],
    "description": "基于Google Protocol Buffer的Buffer拼装器，可以将两段Buffer进行拼装和拆解",
    "autoload": {
        "psr-4": {
            "ZenStudio\\JointBuffer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.4",
        "google/protobuf": "*"
    },
    "minimum-stability": "dev",
    "extra": {
        "hyperf": {
            "config": "ZenStudio\\JointBuffer\\ConfigProvider"
        }
    }
}
